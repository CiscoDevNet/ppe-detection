{
  "name": "ppe",
  "title": "DevNet PPE",
  "version": "0.0.1",
  "description": "",
  "keywords": "DevNet PPE FlowIQ",
  "main": "app/script/main.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/common.js --host local.cisco.com --port 8088 --https ",
    "clean": "rimraf dist",
    "production": "npm run clean && cross-env NODE_ENV=production webpack --config config/webpack/production.js --profile --colors",
    "stage": "npm run clean && cross-env NODE_ENV=stage  webpack --config config/webpack/production.js --profile --colors",
    "integration": "npm run clean && webpack --config config/webpack/production.js --profile",
    "lint": "eslint ./app/script/**/**.js --fix",
    "lint:nofix": "eslint ./app/script/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "author": "Aikepaer Abuduweili <aaikepae@cisco.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less|scss)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "dui": "https://pubhub-lnx.cisco.com/dui/npm/dui-2.0.0.tgz",
    "bootstrap": "^4.0.0",
    "history": "^4.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "4.0.0-beta.12",
    "react-router-dom": "^4.3.1",
    "react-notifications": "latest",
    "prop-types": "^15.7.2",
    "url-parse": "1.4.4",
    "url-toolkit": "2.1.6",
    "socket.io-client": "latest",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.4.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "compression-webpack-plugin": "1.1.11",
    "copy-webpack-plugin": "4.3.0",
    "cross-env": "5.1.2",
    "create-file-webpack": "1.0.0",
    "css-loader": "0.28.10",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.13.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.15.0-rc.8",
    "jest": "22.1.0",
    "node-sass": "4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "strip-loader": "0.1.2",
    "style-loader": "0.20.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.9.6"
  }
}
